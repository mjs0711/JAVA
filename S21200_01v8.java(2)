package after;

public class S21200_01v8 {
    public static void main(String[] args) {
        int intVal  = +100;
        int intVal2 = -100;
        double douVal = +3.14;
        double douVal2 = -3.14;
        
        int intVal1 = 0;
        int intResult1 = -intVal1;
        int intVal21 = 0; 
        int intResult2 = -intVal21;
        double douVal1 = 0.0; 
        double douResult1 = -douVal1;
        double douVal21 = 0.0; 
        double douResult2 = -douVal21;
                        
        System.out.println("intResult1: " + intResult1);
        System.out.println("intResult2: " + intResult2);
        System.out.println("douResult1: " + douResult1);
        System.out.println("douResult2: " + douResult2);
    }
}
