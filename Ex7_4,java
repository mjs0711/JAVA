package ch07;

class point {
	int x, y;

	point(int x, int y) {
		this.x = y;
		this.y = y;
	}
}

class point3D extends point {
	int z;

	point3D(int x, int y, int z) {
		super(x, y); // point(int x, int y)를 호출
		this.z = z;
	}
}

public class Ex7_4 {
	public static void main(String[] args) {
		point3D p = new point3D(1, 2, 3);
		System.out.println("x=" + p.x + ",y=" + p.y + ",z=" + p.z);

	}
}
